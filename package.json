{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "cm": "npx git-cz"
  },
  "dependencies": {
    "@weknow/gatsby-remark-twitter": "^0.2.3",
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^2.24.55",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.4.17",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.26",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-remark-copy-linked-files": "^2.3.14",
    "gatsby-remark-embed-video": "^3.0.10",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.3.29",
    "gatsby-remark-prismjs": "^3.5.11",
    "gatsby-source-filesystem": "^2.3.28",
    "gatsby-transformer-remark": "^2.8.34",
    "gatsby-transformer-sharp": "^2.5.14",
    "intersection-observer": "^0.11.0",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write"
    ]
  }
}
